{
  "name": "@inlustra/env-args",
  "version": "1.0.2",
  "description": "A simple npm package that loads configuration using .env files, environment variables and node arguments",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf ./dist && tsc",
    "test": "npm run build && ava dist/*.spec.js",
    "start": "npm run build && node example/readme.js --alsoRequired='Fixed' --aNumber='4000' --overridden=wooh!",
    "prepublish": "npm run test"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "env",
    "environment",
    "arguments",
    "minimist",
    "configuration"
  ],
  "author": "Inlustra",
  "license": "MIT",
  "dependencies": {
    "chalk": "^2.3.0",
    "change-case": "^3.0.1",
    "dotenv": "^4.0.0",
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "@types/node": "^9.3.0",
    "ava": "^0.24.0",
    "ava-ts": "^0.24.0",
    "ts-node": "^4.1.0",
    "typescript": "^2.6.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Inlustra/env-args.git"
  },
  "homepage": "https://github.com/Inlustra/env-args"
}
